	"tokenColors": [
    {
      "name": "Global settings",
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation, constant.other.symbol",
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Punctuation String",
      "scope": "punctuation.definition.string",
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Punctuation Decorator",
      "scope": "punctuation.decorator",
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "String Escape",
      "scope": "constant.character.escape, text.html constant.character.entity.named",
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "Boolean",
      "scope": [
				"constant.language.boolean",
				"support.type.primitive"
			],
      "settings": {
        "foreground": "#C792EA",
				"fontStyle": "italic"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "Variable",
      "scope": "variable, variable.parameter, support.variable, variable.language, support.constant, meta.definition.variable entity.name.function, meta.function-call.arguments",
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "Variable Method",
      "scope": "meta.method.declaration meta.object.member variable.other.property",
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Other Keyword",
      "scope": "keyword.other",
      "settings": {
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword, modifier, variable.language.this, support.type.object, constant.language",
      "settings": {
        "foreground": "#C792EA",
				"fontStyle": ""
      }
    },
    {
      "name": "Keyword Type Anotation",
      "scope": "keyword.operator.type.annotation",
      "settings": {
        "foreground": "#89DDFF",
				"fontStyle": ""
      }
    },
    {
      "name": "Keyword Optional",
      "scope": "keyword.operator.optional",
      "settings": {
        "foreground": "#89DDFF",
				"fontStyle": "bold"
      }
    },
		{
			"name": "Logical Operators",
			"scope": [
				"keyword.operator.logical"
			],
			"settings": {
				"foreground": "#89DDFF",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Ternary Operators",
			"scope": [
				"keyword.operator.ternary"
			],
			"settings": {
				"foreground": "#89DDFF",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Arithmetic Operators",
			"scope": [
				"keyword.operator.arithmetic",
				"keyword.operator.assignment"
			],
			"settings": {
				"foreground": "#89DDFF",
				"fontStyle": ""
			}
		},
		{
			"name": "Punctuation Separators",
